# Setup QTDIR.
export QTDIR=/cygdrive/e/kyan/ws/avatar-pc/branches/Qt/vs2008-4.6.4

QT_BIN=$QTDIR/bin

# Clear the path first, Setup new path.
export PATH=$QT_BIN:${PATH/$QT_BIN:/}

# Unset local vars.
unset QT_BIN
